Proceso tutoriaPablo
	
	Definir a, b Como Entero;
	
	Escribir "Ingrese numero 1";
	Leer a
	
	Escribir "Ingrese numero 2";
	Leer b
	
	Escribir "Suma: ", a + b;
	Escribir "Resta: ", a - b;
	Escribir "Multiplicación: ", a * b
	// Condición para dividir
	Si b <> 0 Entonces // Distinto: ! =
		Escribir "División: ", a / b
	Sino
		Escribir "No se puede dividir por 0 webon";
	FinSi

FinProceso
