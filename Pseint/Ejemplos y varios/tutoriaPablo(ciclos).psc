Proceso tutoriaPablo
	Definir numeroIndice Como Entero;
	
	numeroIndice = 0;
	
	// Ciclo nos va a imprimir el numero mientras este
	// sea menor o igual (< =) que 10
	Mientras numeroIndice <= 10 Hacer
		
		Escribir "Numero: ", numeroIndice;
		
		numeroIndice = numeroIndice + 1;
		
	FinMientras
	
	
FinProceso
