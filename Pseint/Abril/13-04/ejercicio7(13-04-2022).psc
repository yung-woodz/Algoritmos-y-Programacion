Algoritmo ejercicio7
	
	Definir a, b Como Real;
	
	Escribir "Escribe dos números para comparar";
	Leer a, b;
	
	Si a > b Entonces
		Mostrar a " es mayor que " b;
	FinSi
	
	Si a < b Entonces
		Mostrar a " es menor que " b;
	FinSi
	
	Si a == b Entonces
		Mostrar "Ambos números son iguales";
	FinSi
	
FinAlgoritmo
	