Algoritmo ejercicio23impar
	
	Definir n, impar, promedio, sucesion Como Real;
	
	Escribir "Ingrese un valor de N: ";
	Leer n;
	
	impar <- 1 + 2*(n-1)
	
	Mostrar impar;
	
	sucesion <- (n/2)*(1+impar)
	
	Mostrar sucesion;
	
	promedio <- sucesion / n
	
	Mostrar promedio;
	
FinAlgoritmo	